{"version": 2, "builds": [{"src": "package.json", "use": "@vercel/static-build", "config": {"distDir": "dist"}}], "rewrites": [{"source": "/(.*)", "destination": "/index.html"}], "headers": [{"source": "/static/(.*)", "headers": [{"key": "cache-control", "value": "s-maxage=31536000,immutable"}]}, {"source": "/(.*)", "headers": [{"key": "X-Frame-Options", "value": "DENY"}, {"key": "X-Content-Type-Options", "value": "nosniff"}, {"key": "X-XSS-Protection", "value": "1; mode=block"}]}]}
